{
  "name": "microfe-esm-browser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently npm:dev:*",
    "dev:shell": "vite --port 3000",
    "dev:app-1": "vite app-1 --port 3001",
    "dev:app-2": "vite app-2 --port 3002",
